{
  "name": "v2",
  "description": "Personal Website V2",
  "version": "1.0.0",
  "author": "Jonathan Heyman <heyman.jonathan@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "NODE_OPTIONS=--openssl-legacy-provider gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "--openssl-legacy-provider npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "extract": "yarn run babel --config-file babel-extract.config.js -o tmp/chunk.js 'src/**/*.{js,jsx,ts,tsx}' && rm -rf tmp"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/styled": "^11.1.5",
    "animejs": "^3.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "gatsby": "^5.13.5",
    "gatsby-image": "^2.2.34",
    "gatsby-plugin-dark-mode": "^1.1.2",
    "gatsby-plugin-emotion": "^8.13.1",
    "gatsby-plugin-google-analytics": "^2.1.29",
    "gatsby-plugin-manifest": "^5.13.1",
    "gatsby-plugin-netlify": "^5.1.1",
    "gatsby-plugin-offline": "^6.13.2",
    "gatsby-plugin-react-helmet": "^6.13.1",
    "gatsby-plugin-react-i18next": "^3.0.1",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^5.13.1",
    "gatsby-plugin-sitemap": "^6.13.1",
    "gatsby-plugin-styled-components": "^6.13.1",
    "gatsby-remark-emojis": "^0.4.3",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^7.13.1",
    "gatsby-remark-prismjs": "^7.13.1",
    "gatsby-source-filesystem": "^5.13.1",
    "gatsby-transformer-remark": "^6.13.1",
    "gatsby-transformer-sharp": "^5.13.1",
    "googleapis": "^67.1.1",
    "i18next": "^23.11.5",
    "lodash": "^4.17.21",
    "node": "^18.20.3",
    "prismjs": "^1.21.0",
    "prop-types": "^15.7.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^14.1.2",
    "react-masonry-css": "^1.0.14",
    "react-switch": "^6.0.0",
    "react-transition-group": "^4.3.0",
    "react-tsparticles": "^2.12.2",
    "scrollreveal": "^4.0.5",
    "smooth-scroll": "^16.1.0",
    "styled-components": "^6.1.11",
    "use-sound": "^2.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.6",
    "@babel/plugin-transform-typescript": "^7.24.6",
    "@upstatement/eslint-config": "0.4.3",
    "@upstatement/prettier-config": "^0.3.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-i18next-extract": "^0.10.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "gatsby-remark-code-titles": "^1.1.0",
    "husky": "^4.2.3",
